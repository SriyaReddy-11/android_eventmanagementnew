<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eventmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EventManagement">
        <activity android:name=".EventSubcribeActivity"></activity>
        <activity android:name=".EventRemovedActivity" />
        <activity android:name=".EventUpdatedActivity" />
        <activity android:name=".EventSuccessActivity" />
        <activity android:name=".OrganiserForgotActivity" />
        <activity android:name=".CustomerForgotActivity" />
        <activity
            android:name=".OrganizerRegisterActivity"
            android:exported="false" />
        <activity android:name=".OrganiserEventDetailsActivity" />
        <activity android:name=".GuestsListActivity" />
        <activity android:name=".CustomerProfileActivity" />
        <activity android:name=".OrganiserProfileActivity" />
        <activity android:name=".OrganiserEventsActivity" />
        <activity android:name=".AddEventActivity" />
        <activity
            android:name=".OrganiserHomeActivity"
            android:theme="@style/AppTheme.noactionbar" />
        <activity android:name=".OrganiserLoginActivity" />
        <activity
            android:name=".UserHomeActivity"
            android:theme="@style/AppTheme.noactionbar" />
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".MyEventsActivity" />
        <activity android:name=".ContactUsActivity" />
        <activity android:name=".EventsListActivity" />
        <activity android:name=".CustomerRegistrationActivity" />
        <activity android:name=".CustomerLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashScreenActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>